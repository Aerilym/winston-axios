  },
  "homepage": "https://github.com/Aerilym/winston-axios",
  "devDependencies": {
    "@tsconfig/node16": "^1.0.3",
    "@types/node": "^18.7.8",
    "jsdoc-to-markdown": "^7.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "winston-transport": "^4.5.0"
